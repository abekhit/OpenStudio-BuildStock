      <name>run_start_date</name>
      <display_name>Simulation Start Date</display_name>
      <type>String</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>January 1 2012</default_value>
    </argument>
    <argument>
      <name>run_end_date</name>
      <display_name>Simulation End Date</display_name>
      <type>String</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>December 31 2012</default_value>
    </argument>
  </arguments>
